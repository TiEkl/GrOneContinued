    "mongoose": "^5.2.12",
    "morgan": "^1.9.1",
    "portscanner": "^2.2.0",
    "request": "^2.88.0",
    "roundround": "^0.2.0",
    "svg": "^0.1.0",
    "traverse": "^0.6.6",
    "underscore": "^1.9.1",
    "uniqid": "^5.0.3",
    "vue": "^2.5.21",
    "vue-router": "^3.0.1",
    "xml2js": "^0.4.19",
    "xmlhttprequest": "^1.8.0"
  },
